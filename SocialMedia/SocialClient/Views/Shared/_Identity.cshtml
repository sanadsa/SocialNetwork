@model Client.Models.UserIdentity

<form>
    <div class="form-group">
        <label for="FullName">Full Name</label>
        <p>@Model.FirstName @Model.LastName</p>
    </div>
    <div class="form-group">
        <label for="age">Age</label>
        <p>@Model.Age</p>
    </div>
    <div class="form-group">
        <label for="address">Address</label>
        <p class="help">@Model.Address</p>
    </div>
    <div class="form-group">
        <label for="work">Work Address</label>
        <p>@Model.WorkAddress</p>
    </div>
</form>

<button class="btn">
    @Html.ActionLink("Edit", "Info", "Identity",
    routeValues: new {email = Model.Email},
    htmlAttributes: null)
</button>
