@model WebSite_SocialNetwork.Models.User
@{
    ViewBag.Title = "Wall";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container" style="margin-top:30px">
    @using (Html.BeginForm("GetProfile", "Profile", new { email = Model.Email }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
    {
        <input type="submit" value="View Profile" class="buttonT" />
    }
    @Html.Partial("_Search")
    @Html.Partial("_IdentityPartial")
    @Html.Partial("_PostPartial")
</div>

